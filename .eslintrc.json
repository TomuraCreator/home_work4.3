{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement",
      "no-console"
    ],
    "import/extensions": [
      "error",
      "ignorePackages"
    ]
  }
}